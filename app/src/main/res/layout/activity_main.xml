<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_test_rx_create"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_rx_create" />

    <Button
        android:id="@+id/btn_test_rx_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_rx_map" />

    <Button
        android:id="@+id/btn_test_rx_zip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_rx_zip" />

    <Button
        android:id="@+id/btn_test_rx_concat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_rx_concat" />

    <Button
        android:id="@+id/btn_test_rx_flatmap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_rx_flatmap" />

    <Button
        android:id="@+id/btn_test_rx_concatmap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_rx_concatmap" />

</LinearLayout>